package Swing;
import java.awt.event.ActionListener;

import javax.swing.JButton;

public class TestButton extends JButton 
{
	TestButton(AbstractResponse abstractHandler,String defaultButtonText)
	{
		EventHandler e = new EventHandler(abstractHandler);
		this.setText(defaultButtonText);
		//Button button = new Button(text);//creating instance of JButton  
		addActionListener(e);
	}
	
	
}
