package DigitToWordsExample;

public class IfConversion 
{
	public void digitToWord(int value)
	{
		String answer ="";
		if(value>=1000)
		{
			answer
		}
	}
	
	public String ones(int value)
	{
		String word ="";
		switch(value)
		{
		case 1: word =  "one";
		break;
		case 2: word = "two" ;
		break;
		case 3: word =  "three";
		break;
		case 4: word =  "four";
		break;
		case 5: word = "five";
		break;
		case 6: word =  "six";
		break;
		case 7: word =  "seven";
		break;
		case 8: word = "eight";
		break;
		case 9: word = "nine";
		break;
		case 10: word =  "ten";
		break;
		case 2: word = "two" ;
		break;
		case 3: word =  "three";
		break;
		case 4: word =  "four";
		break;
		case 5: word = "five";
		break;
		case 6: word =  "six";
		break;
		case 7: word =  "seven";
		break;
		case 8: word = "eight";
		break;
		case 9: word = "nine";
		
		}
	}
}
