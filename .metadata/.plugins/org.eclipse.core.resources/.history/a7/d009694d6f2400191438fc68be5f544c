package ReverseString;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		Reverse reverse = new Reverse();
		if(sc.hasNext())
		{
			String input = sc.nextLine();
			if(input!="")
			{
				reverse.reverseTheWorld(input);
			}
		}
		
	}

	

}
