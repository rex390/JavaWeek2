import java.util.Scanner;

public class InputClass 
{
	Scanner sc;
	InputClass()
	{
		sc = new Scanner(System.in);
	}
	
	public int getInput()
	{
		String input = sc.nextLine();
		return 0;
		
		
	}
	public boolean stringToInteger(String input)
	{
		int parsedVal;
		try
		{
			parsedVal = Integer.parseInt(input)
			
		}
		return false;		
	}
}
